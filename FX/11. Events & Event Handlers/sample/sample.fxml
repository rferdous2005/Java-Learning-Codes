
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<TilePane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" vgap="20" hgap="20"
          stylesheets="@Style.css">

    <Label fx:id="allItems" prefWidth="200">

    </Label>

    <TextField fx:id="itemName" prefWidth="100" onKeyReleased="#keyRealeased"/>

    <Button fx:id="btn1" text="Add Item" onAction="#onAddButtonClicked"/>

    <Button fx:id="btn2" text="Delete" onAction="#onAddButtonClicked"/>

















</TilePane>